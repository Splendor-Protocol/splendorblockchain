		"devdoc": {
			"details": "Contract for managing blocklisted/paused wallets to prevent bad actors from transferring native coins",
			"kind": "dev",
			"methods": {
				"addToBlocklist(address,string)": {
					"details": "Add a wallet to the blocklist",
					"params": {
						"reason": "The reason for blocklisting",
						"wallet": "The address to blocklist"
					}
				},
				"addToBlocklistBatch(address[],string[])": {
					"details": "Add multiple wallets to the blocklist in batch",
					"params": {
						"reasons": "Array of reasons for blocklisting (must match wallets length)",
						"wallets": "Array of addresses to blocklist"
					}
				},
				"clearBlocklist()": {
					"details": "Clear the entire blocklist (emergency function)"
				},
				"getAllBlocklisted()": {
					"details": "Get all blocklisted addresses",
					"returns": {
						"_0": "Array of all blocklisted addresses"
					}
				},
				"getBlocklistCount()": {
					"details": "Get the total number of blocklisted addresses",
					"returns": {
						"_0": "The count of blocklisted addresses"
					}
				},
				"getBlocklistInfo(address)": {
					"details": "Get blocklist information for a wallet",
					"params": {
						"wallet": "The address to query"
					},
					"returns": {
						"isBlocked": "True if the wallet is blocklisted",
						"reason": "The reason for blocklisting",
						"timestamp": "When the wallet was blocklisted"
					}
				},
				"getBlocklistedPaginated(uint256,uint256)": {
					"details": "Get blocklisted addresses with pagination",
					"params": {
						"limit": "Number of addresses to return",
						"offset": "Starting index"
					},
					"returns": {
						"_0": "Array of blocklisted addresses"
					}
				},
				"initialize()": {
					"details": "Constructor - automatically sets the hardcoded admin address"
				},
				"isBlocklisted(address)": {
					"details": "Check if a wallet is blocklisted",
					"params": {
						"wallet": "The address to check"
					},
					"returns": {
						"_0": "bool True if the wallet is blocklisted"
					}
				},
				"removeFromBlocklist(address)": {
					"details": "Remove a wallet from the blocklist",
					"params": {
						"wallet": "The address to remove from blocklist"
					}
				},
				"removeFromBlocklistBatch(address[])": {
					"details": "Remove multiple wallets from the blocklist in batch",
					"params": {
						"wallets": "Array of addresses to remove from blocklist"
					}
				}
			},
			"title": "WalletBlocklist",
			"version": 1
		},
